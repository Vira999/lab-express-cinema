<div class="container">
  <div class="row text-center">
    {{#each movies}}
    <div class="col-md-4">
      <a href="/movie/{{this._id}}">
        <img src="{{this.image}}" alt="" class="ima">
        <p class="mt-2">{{this.title}}</p>
      </a>
    </div>
    {{/each}}
  </div>
</div>